{
  "version": 2,
  "env": {
    "MONGODB_URI": "@landing-mongodb-uri",
    "GOOGLE_MAPS_API_KEY": "@landing-google-maps-api",
    "GOOGLE_MAPS_API_URL": "https://maps.googleapis.com/maps/api",
    "IPIFY_URL": "https://api.ipify.org",
    "IPGEOLOCATION_API_KEY": "@ipgeolocation-api",
    "IPGEOLOCATION_API_URL":"https://api.ipgeolocation.io/",
    "PUSHER_APP_ID":"@landing-pusher-app-ip",
    "PUSHER_APP_KEY":"@landing-pusher-app-key",
    "PUSHER_APP_SECRET":"@landing-pusher-app-secret",
    "PUSHER_APP_CLUSTER":"@landing-pusher-app-cluster",
    "AUTH0_DOMAIN":"@landing-auth0-domain",
    "AUTH0_CLIENT_ID":"@landing-auth0-cliend-id"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        },
        {
            "key":"Access-Control-Allow-Headers",
            "value":"*"
        }
      ]
    }
  ]
}